{
  "author": "Hartorn",
  "name": "dd5-character-builder",
  "description": "Character builder for D&D 5",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hartorn/dd5-character-builder.git"
  },
  "engines": {
    "node": "6.9.x",
    "npm": "3.10.x"
  },
  "scripts": {
    "bundle": "better-npm-run bundle",
    "build": "npm run bundle",
    "deploy": "node ./deploy.js",
    "start": "npm run start:npm",
    "start:npm": "better-npm-run webpack-dev-server-npm-focus",
    "watch": "better-npm-run watch"
  },
  "main": "app/index.js",
  "babel": {
    "presets": [
      "focus"
    ]
  },
  "betterScripts": {
    "bundle": {
      "command": "webpack",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "D&D 5 - Character builder",
        "OUTPUT_DIR": "./docs",
        "OUTPUT_PUBLIC_PATH": "./",
        "GENERATE_HTML": false,
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "DEV": false,
        "MINIMIFY": true,
        "SOURCE_MAPS": false,
        "node_env": "production",
        "BABEL_ENV": "production",
        "BASE_URL": "/"
      }
    },
    "watch": {
      "command": "webpack --watch",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    },
    "webpack-dev-server-npm-focus": {
      "command": "node ./dev-server.js",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    }
  },
  "devDependencies": {
    "animate.css": "^3.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.4",
    "babel-preset-focus": "^0.5.2",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.5",
    "bitballoon": "^0.2.2",
    "body-parser": "^1.14.1",
    "css-loader": "^0.21.0",
    "eslint": "1.5.1",
    "eslint-config-focus": "0.3.0",
    "eslint-plugin-filenames": "0.1.1",
    "eslint-plugin-react": "3.5.0",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "focus-components": "2.0.0",
    "focus-core": "2.0.0",
    "focus-devtools": "0.4.0",
    "font-awesome": "^4.5.0",
    "json-loader": "^0.5.3",
    "lodash": "^3.10.1",
    "material-design-icons-iconfont": "^2.0.4",
    "migration-tool": "^0.1.0",
    "moment": "^2.16.0",
    "native-promise-only": "^0.8.0-a",
    "numeral": "^1.5.3",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-date-picker": "^3.1.10",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-focus": "^0.12.2"
  }
}
