{
  "author": "Hartorn",
  "name": "dd5-character-builder",
  "description": "Character builder for D&D 5",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hartorn/dd5-character-builder.git"
  },
  "engines": {
    "node": "6.9.x",
    "npm": "3.10.x"
  },
  "scripts": {
    "bundle": "better-npm-run bundle",
    "build": "npm run bundle",
    "start": "npm run start:npm",
    "start:npm": "better-npm-run webpack-dev-server-npm-focus",
    "watch": "better-npm-run watch"
  },
  "main": "app/index.js",
  "babel": {
    "presets": [
      "focus"
    ]
  },
  "betterScripts": {
    "bundle": {
      "command": "webpack",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "D&D 5 - Character builder",
        "OUTPUT_DIR": "./docs",
        "OUTPUT_PUBLIC_PATH": "./",
        "GENERATE_HTML": false,
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "DEV": false,
        "MINIMIFY": true,
        "SOURCE_MAPS": false,
        "node_env": "production",
        "BABEL_ENV": "production",
        "BASE_URL": "/character-builder-dd5/"
      }
    },
    "watch": {
      "command": "webpack --watch",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    },
    "webpack-dev-server-npm-focus": {
      "command": "node ./dev-server.js",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": false,
        "LEGACY_SEARCH_API": false,
        "BASE_URL": "/"
      }
    }
  },
  "devDependencies": {
    "animate.css": "^3.5.2",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-focus": "^0.6.1",
    "babel-runtime": "^6.20.0",
    "better-npm-run": "0.0.13",
    "body-parser": "^1.15.2",
    "css-loader": "^0.26.1",
    "eslint": "3.12.2",
    "eslint-config-focus": "0.4.0",
    "eslint-plugin-filenames": "1.1.0",
    "eslint-plugin-react": "6.8.0",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "focus-components": "2.1.1",
    "focus-core": "2.1.1",
    "focus-devtools": "0.4.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.17.1",
    "native-promise-only": "^0.8.1",
    "numeral": "^1.5.6",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-date-picker": "^5.3.28",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.0",
    "react-slick": "^0.14.5",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.1.1",
    "slick-carousel": "^1.6.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-focus": "^0.12.2"
  }
}
